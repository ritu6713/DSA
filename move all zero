public class moveallzero {

    public static void main(String[] args) {
       
        int a[] ={1,0,4,0,0,6,7};
        int count =0;

        for (int i = 0; i < a.length; i++) {
            if ( a[i] !=0) {
                int temp=a[i];
                a[i]=a[count];
                a[count]=temp;
                count++;
            }
        }
        for (int i = 0; i < a.length; i++) {
            System.out.println(a[i]);
        }

    }
    
}
