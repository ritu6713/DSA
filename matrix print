public class print2DArray {
    
   static void print(int a[][]){
      for (int i = 0; i < a.length; i++) {
          for (int j = 0; j < a[i].length; j++) {
            System.out.print(a[i][j]);
          }
          System.out.println();
      }
   }
    
    
    
    
    
    
    
    public static void main(String[] args) {
        int a[][]={{1,2,3},{4,5,6}};
        print(a);
    }
}
